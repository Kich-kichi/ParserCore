{
  "name": "fingli/parser-core",
  "type": "library",
  "description": "PHP news parser core",
  "keywords": ["parser", "news", "html", "rss"],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Goncharenya",
      "email": "goncharenya@gmail.com"
    },
    {
      "name": "FingliGroup",
      "homepage": "https://fingli.ru"
    }
  ],
  "require": {
    "php": ">=7.4",
    "linslin/yii2-curl": "^1.4",
    "cagartner/phpquery": "^0.9.12",
    "lanfix/parser": "*",
    "symfony/dom-crawler": "^5.1",
    "rowbot/punycode": "^1.0",
    "ext-dom": "*",
    "symfony/css-selector": "^5.1",
    "wapmorgan/time-parser": "^2.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.1"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "autoload": {
    "psr-4": {
      "fingli\\ParserCore\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-version": "1.1"
  }
}
